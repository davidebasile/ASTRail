		<transition>
			<source ref="id0"/>
			<target ref="id0"/>
			<label kind="guard" x="-238" y="-153">c1&gt;=freq_req</label>
			<label kind="synchronisation" x="-238" y="-136">LocationRequest!</label>
			<label kind="assignment" x="-238" y="-119">c1=0</label>
			<nail x="-255" y="-153"/>
			<nail x="-255" y="-77"/>
			<nail x="-332" y="-77"/>
		</transition>
